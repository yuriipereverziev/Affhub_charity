extends settings/layout

prepend html
    - const lang = "uk";

prepend head
    - const pageTitle = "Meetup for Ukrainian Affiliate Community";
    - const themeColor = "#fbfbfb";

append head

append content
    .custom-bg.promo-bg
        span.top-gradient-item
        .bg
            include components/promo
            include components/planing

    include components/purpose

    .separator

    .custom-bg.lots-bg.pink
        .bg
            include components/lots

    include components/connect
    .custom-bg.partner
        .bg
            include components/partners

    .custom-bg.sponsor-bg.pink.turquoise
        .bg
            include components/sponsors
            include components/media-partners

    .separator

    .custom-bg.team-bg.yellow
        .bg
            include components/team

    include components/registration
    include components/map
    include components/media

    include components/footer

    include components/popup

append scripts
